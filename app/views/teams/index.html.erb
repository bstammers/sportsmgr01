<h1>Listing teams</h1>

<table>
  <thead>
    <tr>
      <th>Teamname</th>
      <th>Competition</th>
      <th>Division</th>
      <th>Teammgr</th>
      <th>Played</th>
      <th>Won</th>
      <th>Lost</th>
      <th>Tied</th>
      <th>Sets won</th>
      <th>Sets lost</th>
      <th>Points for</th>
      <th>Points against</th>
      <th>Sets percent</th>
      <th>Points percent</th>
      <th>Comp points</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team.teamname %></td>
        <td><%= team.competition_id %></td>
        <td><%= team.division_id %></td>
        <td><%= team.teammgr %></td>
        <td><%= team.played %></td>
        <td><%= team.won %></td>
        <td><%= team.lost %></td>
        <td><%= team.tied %></td>
        <td><%= team.sets_won %></td>
        <td><%= team.sets_lost %></td>
        <td><%= team.points_for %></td>
        <td><%= team.points_against %></td>
        <td><%= team.sets_percent %></td>
        <td><%= team.points_percent %></td>
        <td><%= team.comp_points %></td>
        <td><%= link_to 'Show', team %></td>
        <td><%= link_to 'Edit', edit_team_path(team) %></td>
        <td><%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team', new_team_path %>
